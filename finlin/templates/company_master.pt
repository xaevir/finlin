<html metal:use-macro="main">
  <div metal:fill-slot="body" id="company">
    <div id="hd">
        <h1>${context.data['name']}<span>(${context.data['ticker']})</span></h1>
        <ul id="company-nav" class="clearfix">
            <li tal:repeat="(url, label) context.nav">
        
                <a href="${url}">${label}</a>
            </li>
        </ul>
    </div>

    <div metal:define-slot="company-page">
        Company Page goes here
    </div>


  <div metal:fill-slot="footer-js" id="footer-js">
    <div metal:define-slot="footer-js">
    </div>
  </div>
  </div>
</html>

